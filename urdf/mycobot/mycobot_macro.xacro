           <mesh filename="package://mycobot_ros/urdf/mycobot/joint5.dae"/>
          </geometry>
          <origin xyz = "0.0 0 -0.03356 " rpy = " 0 -1.5708 1.5708"/>
        </collision>
      </link>


      <link name="${prefix}joint6">
