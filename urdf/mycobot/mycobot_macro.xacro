          </geometry>
          <origin xyz = "0.0 0 -0.06096 " rpy = " 0 0 -1.5708"/>
        </visual>
        <collision>
         <geometry>
             <mesh filename="package://mycobot_ros/urdf/mycobot/joint2.dae"/>
          </geometry>
          <origin xyz = "0.0 0 -0.06096 " rpy = " 0 0 -1.5708"/>
