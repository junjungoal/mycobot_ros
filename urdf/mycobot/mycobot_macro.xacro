<?xml version='1.0' encoding='utf-8'?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="firefighter">
    <xacro:macro name="mycobot" params="prefix:=''">
      <xacro:property name="width" value=".2" />

      <link name="${prefix}joint1">
